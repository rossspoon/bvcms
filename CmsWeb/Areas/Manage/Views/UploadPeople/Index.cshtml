@using CmsWeb
@{
    Layout = "/Views/Shared/SiteLayout.cshtml";
    ViewBag.Title = "UploadPeople";
}
<h2>Upload People</h2>
<form method="POST" action="/UploadPeople/Upload">
    @Html.TextArea("text", ViewData["text"].ToString(), new { rows = "30", cols = "1000", wrap = "off", style = "width:90%" })<br />
    <input type="submit" class="bt" value="Submit" />
</form> 